{
  "name": "optimistdigital/nova-sortable",
  "description": "This Laravel Nova package allows you to reorder models in a Nova resource's index view using drag & drop.",
  "keywords": [
    "laravel",
    "nova",
    "eloquent-sortable",
    "optimistdigital"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.3.0",
    "spatie/eloquent-sortable": "^3.10.0|^4.0",
    "laravel/nova": "^3.0",
    "optimistdigital/nova-translations-loader": "dev-nova4"
  },
  "autoload": {
    "psr-4": {
      "OptimistDigital\\NovaSortable\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "OptimistDigital\\NovaSortable\\ToolServiceProvider"
      ]
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:tominal/nova-translations-loader"
    }
  ],
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
